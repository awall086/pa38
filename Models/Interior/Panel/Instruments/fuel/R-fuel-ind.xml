<?xml version="1.0"?>

<PropertyList>

 <path>R-fuel-ind.ac</path>

	<animation>
		<type>material</type>
		<object-name>R-fuel-case</object-name>
		<object-name>R-fuel-needle</object-name>
		<emission>
			<red>1.00</red>
			<green>0.65</green>
			<blue>0.25</blue>
			<factor-prop>/sim/model/pa38/lighting/instruments</factor-prop>
		</emission>
	</animation>

    <animation>
        <type>noshadow</type>
		<object-name>R-fuel-glass</object-name>
	</animation>

 <animation>
  <type>rotate</type>
  <object-name>R-fuel-needle</object-name>
  <property>consumables/fuel/tank[2]/level-gal_us</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>5</ind>
    <dep>-32</dep>
   </entry>
   <entry>
    <ind>10</ind>
    <dep>-62</dep>
   </entry>
   <entry>
    <ind>16</ind>
    <dep>-87</dep>
   </entry>
  </interpolation>
  <center>
   <x-m>0.000</x-m>
   <y-m>-0.01206</y-m>
   <z-m>0.000</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


