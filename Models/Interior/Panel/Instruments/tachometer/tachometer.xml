<?xml version="1.0"?>

<PropertyList>

 <path>tachometer.ac</path>

    <params>
        <engine>
            <rpm>engines/engine[0]/rpm</rpm>
        </engine>
    </params>

	<animation>
		<type>material</type>
  <object-name>Face</object-name>
  <object-name>needle</object-name>
		<emission>
			<red>1.00</red>
			<green>0.65</green>
			<blue>0.25</blue>
			<factor-prop>/sim/model/pa38/lighting/instruments</factor-prop>
		</emission>
	</animation>

 <animation> 
  <type>rotate</type>
  <object-name>needle</object-name>
  <property alias="/params/engine/rpm"/>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>3500</ind><dep>270</dep></entry>
  </interpolation>
  <axis> 
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>  
  </center>
 </animation>         
 
</PropertyList>
