<!-- VOR 1 Radial adjust -->

<animation>
    <type>knob</type>
    <object-name>radial_knob</object-name>
    <property>/instrumentation/nav[0]/radials/selected-deg</property>
    <factor>-8</factor>

    <axis>
        <x>1</x>
        <y>0</y>
        <z>0</z>
    </axis>
    <center>
        <x-m>0.012884</x-m>
        <y-m>-0.034772</y-m>
        <z-m>-0.038662</z-m>
    </center>

    <shift-repeat type="int">5</shift-repeat>
     
    <action>
        <binding>
            <command>property-adjust</command>
            <property>/instrumentation/nav[0]/radials/selected-deg</property>
            <factor>1</factor>
            <min>0</min>
            <max>360</max>
            <wrap>1</wrap>
        </binding>
			<binding>
				<command>nasal</command>
				<script>pa38.click("dial")</script>
			</binding>
    </action>
    
    <hovered>
     <binding>
       <command>set-tooltip</command>
       <tooltip-id>nav1-obs</tooltip-id>
       <label>Nav 1 Radial %3d</label>
       <mapping>heading</mapping>
       <property>/instrumentation/nav[0]/radials/selected-deg</property>
    </binding>
    </hovered>
 </animation>
     
  <!-- Nav part -->

  <animation>
    <type>textranslate</type>
    <object-name>NavUse10M</object-name>
    <property alias="../../params/nav-frq-selected"/>
    <step>10</step>
    <factor>0.01</factor>
    <bias>0.0001</bias>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>NavUse1M</object-name>
    <property alias="../../params/nav-frq-selected"/>
    <step>1</step>
    <factor>0.1</factor>
    <bias>0.0001</bias>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>NavUse100K</object-name>
    <property alias="../../params/nav-frq-selected"/>
    <step>0.1</step>
    <factor>1</factor>
    <bias>0.0001</bias>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>NavUse10K</object-name>
    <property alias="../../params/nav-frq-selected"/>
    <step>0.01</step>
    <factor>10</factor>
    <bias>0.0001</bias>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

<!-- actions -->

  <animation>
    <type>knob</type>
    <object-name>NavVolume</object-name>
    <property alias="../../params/nav-volume"/>
    <factor>-280</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.012884</x-m>
      <y-m>0.038072</y-m>
      <z-m>0.037536</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="../../../../params/nav-volume"/>
        <factor>0.1</factor>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>false</wrap>
      </binding>
			<binding>
				<command>nasal</command>
				<script>pa38.click("dial")</script>
			</binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <tooltip-id>nav-volume</tooltip-id>
        <label>Volume: %3d%%</label>
        <property alias="../../../../params/nav-volume"/>
        <mapping>percent</mapping>
      </binding>
    </hovered>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>NavKHz</object-name>
    <property alias="/params/nav-dial-khz"/>
    <factor>-0.25</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.027751</x-m>
      <y-m>0.036462</y-m>
      <z-m>-0.039199</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/nav-frq-selected"/>
        <factor>0.05</factor>
        <min>0.0</min>
        <max>1.0</max>
        <resolution>0.050</resolution>
        <wrap>true</wrap>
        <mask>decimal</mask>
      </binding>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/nav-dial-khz"/>
        <factor>4</factor>
        <min>0</min>
        <max>360</max>
        <wrap>true</wrap>
      </binding>
			<binding>
				<command>nasal</command>
				<script>pa38.click("dial")</script>
			</binding>
    </action>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>NavMHz</object-name>
    <property alias="/params/nav-dial-mhz"/>
    <factor>-0.25</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.014573</x-m>
      <y-m>0.036462 </y-m>
      <z-m>-0.039199</z-m>
    </center>
    <action>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/nav-frq-selected"/>
        <factor>1</factor>
        <min>108.0</min>
        <max>118.0</max>
        <wrap>true</wrap>
        <mask>integer</mask>
      </binding>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/nav-dial-mhz"/>
        <factor>4</factor>
        <min>0</min>
        <max>360</max>
        <wrap>true</wrap>
      </binding>
			<binding>
				<command>nasal</command>
				<script>pa38.click("dial")</script>
			</binding>
    </action>
  </animation>

<!-- Localizer markers -->

	<animation>
		<object-name>inner_marker</object-name>
		<type>select</type>
		<condition>
            <and>
				<greater-than>
					<property alias="/params/power-nav"/>
					<value>0</value>
				</greater-than>
				<greater-than>
					<property>instrumentation/marker-beacon/imBright</property>
					<value>0</value>
				</greater-than>
			</and>
		</condition>
	</animation>

    <animation>
        <type>material</type>
        <object-name>inner_marker</object-name>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
            <factor-prop>/instrumentation/marker-beacon/imBright</factor-prop>
        </emission>
    </animation>

	<animation>
		<object-name>mid_marker</object-name>
		<type>select</type>
		<condition>
            <and>
				<greater-than>
					<property alias="/params/power-nav"/>
					<value>0</value>
				</greater-than>
				<greater-than>
					<property>instrumentation/marker-beacon/mmBright</property>
					<value>0</value>
				</greater-than>
			</and>
		</condition>
	</animation>
	
    <animation>
        <type>material</type>
        <object-name>mid_marker</object-name>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
            <factor-prop>/instrumentation/marker-beacon/mmBright</factor-prop>
        </emission>
    </animation>

	<animation>
		<object-name>outer_marker</object-name>
		<type>select</type>
		<condition>
            <and>
				<greater-than>
					<property alias="/params/power-nav"/>
					<value>0</value>
				</greater-than>
				<greater-than>
					<property>instrumentation/marker-beacon/omBright</property>
					<value>0</value>
				</greater-than>
			</and>
		</condition>
	</animation>
	
    <animation>
        <type>material</type>
        <object-name>outer_marker</object-name>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
            <factor-prop>/instrumentation/marker-beacon/omBright</factor-prop>
        </emission>
    </animation>
	
