		<center>
			<x-m>0.001478</x-m>
			<y-m>-0.030106</y-m>
			<z-m>0.030602</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>fuel_press_needle</object-name>
		<property>engines/engine[0]/fuel-flow-gph</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.45</ind>
				<dep>32</dep>
			</entry>
			<entry>
				<ind>8</ind>
				<dep>45</dep>
