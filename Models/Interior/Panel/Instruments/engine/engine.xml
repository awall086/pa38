	<animation>
		<type>rotate</type>
		<object-name>oil_temp_needle</object-name>
		<property>engines/engine/oil-temperature-degf</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>75</ind>
				<dep>5</dep>
			</entry>
			<entry>
				<ind>180</ind>
				<dep>28</dep>
			</entry>
			<entry>
				<ind>245</ind>
				<dep>68</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.001478</x-m>
			<y-m>-0.030106</y-m>
			<z-m>-0.027439</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

 
	<animation>
		<type>rotate</type>
		<object-name>oil_press_needle</object-name>
		<property>engines/engine/oil-pressure-psi</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>25</ind>
				<dep>5</dep>
			</entry>
			<entry>
				<ind>60</ind>
				<dep>44</dep>
			</entry>
			<entry>
				<ind>100</ind>
				<dep>68</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.00148</x-m>
			<y-m>0.03011</y-m>
			<z-m>-0.027439</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>alt_amps_needle</object-name>
		<property>systems/pa38/electrical/amps</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>30</ind>
				<dep>27</dep>
