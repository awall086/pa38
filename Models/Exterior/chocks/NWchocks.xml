<?xml version="1.0"?>

<PropertyList>

    <path>NWchocks.ac</path>
    <nopreview/>
	
	  <animation>
   <type>select</type>
   <object-name>chock1</object-name>
   <object-name>chock2</object-name>
   <object-name>cord</object-name>
   <condition>
     <property>sim/model/pa38/chocks/NWchocks</property>
   </condition>
 </animation>

<animation>
   <type>pick</type>
   <object-name>cord</object-name>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
		<command>property-toggle</command>
       <property>sim/model/pa38/chocks/NWchocks</property>
     </binding>
   </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>remove_NWchocks</tooltip-id>
      <label>Remove nose wheel chocks</label>
   </binding>
   </hovered>
</animation>

</PropertyList>
